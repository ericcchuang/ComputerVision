{
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "ui/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/$1.py"
        },
        {
            "src": "/(.*)",
            "dest": "ui/dist/index.html"
        }
    ]
}